{
  "name": "discord-bot-boilerplate",
  "version": "1.0.0",
  "description": "my personal boiler plate for crafting discord bots.",
  "main": "index.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun src/index.ts",
    "deploy": "bun src/utils/deploy.ts",
    "dev": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "bun src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "dfanso",
  "license": "ISC",
  "repository": "https://github.com/DFanso/discord-bot-boilerplate.git",
  "dependencies": {
    "@discordjs/builders": "^1.8.2",
    "@swc/core": "^1.5.25",
    "@types/node": "^20.14.0",
    "discord-api-types": "^0.37.83",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "env": "^0.0.2",
    "eslint": "^9.4.0",
    "express": "^4.19.2",
    "mongoose": "^8.4.1",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@swc-node/register": "^1.9.1",
    "@swc/cli": "^0.3.12",
    "@types/express": "^4.17.21"
  },
  "packageManager": "bun@1.1.13"
}